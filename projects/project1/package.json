{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass main.scss style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass main.scss style.comp.css",
    "prefix:css": "postcss -u autoprefixer -b \"last 10 versions\" -o style.prefix.css style.comp.css",
    "compress:css": "node-sass style.prefix.css style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "taki",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.2.0",
    "concat": "^1.0.3",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  },
  "dependencies": {}
}
